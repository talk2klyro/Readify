{
  "functions": {
    "functions/*.js": {
      "runtime": "nodejs18.x"
    }
  },
  "builds": [
    {
      "src": "functions/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "functions/$1.js"
    }
  ],
  "analytics": {
    "enabled": true
  },
  "storage": {
    "blobs": {
      "driver": "vercel"
    }
  }
}
